# Databricks configuration
DATABRICKS_WORKSPACE_URL=https://adb-1234567890123456.10.azuredatabricks.net
DATABRICKS_TOKEN=your_databricks_token_here

# Unity Catalog configuration
UNITY_CATALOG_ID=

# Azure Storage configuration
AZURE_STORAGE_ACCOUNT_NAME=

# PostgreSQL configuration
PG_INSTANCE_NAME=your-instance-name
PG_USERNAME=your.email@company.com
PG_DATABASE=databricks_postgres
PG_SCHEMA=your.email@company.com

# PostgreSQL instance hostname mappings (JSON format)
PG_INSTANCE_HOSTNAMES={"your-instance-name": "your-instance-hostname.database.azuredatabricks.net"}

# Default Delta table configuration
DEFAULT_DELTA_TABLE=your_catalog.your_schema.your_table
DEFAULT_BUSINESS_KEYS=id,key1,key2

# Table Configuration
TABLE=your_catalog.your_schema.your_table
